      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{9160CC29-D97B-4961-A1FF-1B2087CDAF23}"
          SQLTask:SqlStatementSource="-- ================================================================&#xA;-- ETL: DIM_TIEMPO&#xA;-- ================================================================&#xA;-- 1. Limpiar datos existentes&#xA;DELETE FROM TarjetasDW.dw.DIM_TIEMPO;&#xA;&#xA;-- 2. Generar e insertar fechas&#xA;WITH FechasRecursivas AS (&#xA;    SELECT CAST('2015-01-01' AS DATE) AS Fecha&#xA;    UNION ALL&#xA;    SELECT DATEADD(DAY, 1, Fecha)&#xA;    FROM FechasRecursivas&#xA;    WHERE Fecha &lt; '2090-12-31'&#xA;)&#xA;INSERT INTO TarjetasDW.dw.DIM_TIEMPO (&#xA;    TiempoKey,&#xA;    Fecha, &#xA;    Anio, &#xA;    Mes, &#xA;    Trimestre,&#xA;    NombreMes&#xA;)&#xA;SELECT&#xA;    ROW_NUMBER() OVER (ORDER BY Fecha) AS TiempoKey,&#xA;    Fecha,&#xA;    YEAR(Fecha) AS Anio,&#xA;    MONTH(Fecha) AS Mes,&#xA;    DATEPART(QUARTER, Fecha) AS Trimestre,&#xA;    CASE MONTH(Fecha)&#xA;        WHEN 1 THEN N'Enero'&#xA;        WHEN 2 THEN N'Febrero'&#xA;        WHEN 3 THEN N'Marzo'&#xA;        WHEN 4 THEN N'Abril'&#xA;        WHEN 5 THEN N'Mayo'&#xA;        WHEN 6 THEN N'Junio'&#xA;        WHEN 7 THEN N'Julio'&#xA;        WHEN 8 THEN N'Agosto'&#xA;        WHEN 9 THEN N'Septiembre'&#xA;        WHEN 10 THEN N'Octubre'&#xA;        WHEN 11 THEN N'Noviembre'&#xA;        WHEN 12 THEN N'Diciembre'&#xA;    END AS NombreMes&#xA;FROM FechasRecursivas&#xA;OPTION (MAXRECURSION 0);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
