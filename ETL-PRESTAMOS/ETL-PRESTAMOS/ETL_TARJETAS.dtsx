SELECT 
    s.SucursalId,
    s.Codigo,
    s.Nombre,
    c.Nombre AS Ciudad,
    r.Nombre AS Region,
    p.Nombre AS Pais
FROM BnacoDB2.nucleo.Sucursal s
INNER JOIN BnacoDB2.referencial.Ciudad c ON c.CiudadId = s.CiudadId
INNER JOIN BnacoDB2.referencial.Region r ON r.RegionId = c.RegionId
INNER JOIN BnacoDB2.referencial.Pais p ON p.PaisId = r.PaisId
WHERE NOT EXISTS (
    SELECT 1 
    FROM TarjetasDW.dw.DIM_SUCURSAL d
    WHERE d.SucursalId = s.SucursalId
);</property>
