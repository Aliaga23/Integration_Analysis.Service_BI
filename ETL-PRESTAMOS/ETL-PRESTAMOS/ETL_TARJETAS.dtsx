                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- ================================================================
-- ETL: FACT_TRANSACCIONES_TARJETA (CORREGIDO - SIN DUPLICADOS)
-- ================================================================
SELECT 
    a.AutorizacionId,
    
    -- ========== DIMENSIONES ==========
    dt.TiempoKey,
    dcom.ComercioKey,
    dcli.ClienteKey,
    dp.ProductoKey,
    ds.SucursalKey,
    CASE 
        WHEN cap.Estado = 1 AND cap.MontoCapturado &gt; 0 THEN 1  -- CAPTURADA_EXITOSA
        ELSE 2  -- CAPTURA_RECHAZADA
    END AS EstadoKey,
    
    -- ========== MÉTRICAS ==========
    a.Monto AS MontoAutorizado,
    ISNULL(cap.MontoCapturado, 0) AS MontoCapturado,
    1 AS CantidadTransacciones,
    0 AS TieneDisputa,
    CASE WHEN cap.Estado = 1 AND cap.MontoCapturado &gt; 0 THEN 1 ELSE 0 END AS TransaccionExitosa,
    
    -- ========== CONTROL ==========
    a.AutorizadoEn AS FechaHoraAutorizacion

FROM BnacoDB2.tarjetas.Autorizacion a
LEFT JOIN BnacoDB2.tarjetas.Captura cap ON cap.AutorizacionId = a.AutorizacionId
INNER JOIN BnacoDB2.tarjetas.Tarjeta t ON t.TarjetaId = a.TarjetaId

-- ✅ Solo la PRIMERA cuenta vinculada a la tarjeta (evita duplicados por múltiples cuentas)
INNER JOIN (
    SELECT TarjetaId, CuentaId,
           ROW_NUMBER() OVER (PARTITION BY TarjetaId ORDER BY TarjetaCuentaLinkId) AS rn
    FROM BnacoDB2.tarjetas.TarjetaCuentaLink
) tcl ON tcl.TarjetaId = t.TarjetaId AND tcl.rn = 1

INNER JOIN BnacoDB2.nucleo.Cuenta c ON c.CuentaId = tcl.CuentaId

-- ✅ Solo el PRIMER titular de la cuenta (evita duplicados por múltiples titulares)
INNER JOIN (
    SELECT CuentaId, ClienteId,
           ROW_NUMBER() OVER (PARTITION BY CuentaId ORDER BY CuentaTitularId) AS rn
    FROM BnacoDB2.nucleo.TitularCuenta
) tc ON tc.CuentaId = c.CuentaId AND tc.rn = 1

INNER JOIN BnacoDB2.nucleo.Cliente cl ON cl.ClienteId = tc.ClienteId
INNER JOIN TarjetasDW.dw.DIM_TIEMPO dt ON dt.Fecha = CAST(a.AutorizadoEn AS DATE)
INNER JOIN TarjetasDW.dw.DIM_COMERCIO dcom ON dcom.ComercioId = a.ComercioId
INNER JOIN TarjetasDW.dw.DIM_CLIENTE dcli ON dcli.ClienteId = tc.ClienteId
INNER JOIN TarjetasDW.dw.DIM_PRODUCTO dp ON dp.ProductoId = c.ProductoId
LEFT JOIN TarjetasDW.dw.DIM_SUCURSAL ds ON ds.SucursalId = cl.SucursalPrincipalId

-- ========== FILTRO ANTI-DUPLICADOS EN EJECUCIONES REPETIDAS ==========
WHERE NOT EXISTS (
    SELECT 1 
    FROM TarjetasDW.dw.FACT_TRANSACCIONES_TARJETA ft
    WHERE ft.AutorizacionId = a.AutorizacionId
);</property>
